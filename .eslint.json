{
  "root": true,
  "extends": ["plugin:vue/essential", "eslint:recommend"],
  "globals": {
    "process": "readable"
  },
  "env": {},
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "no-console": ["error", { "allow": ["warn", "error"] }],
      "semi": ["error", "always"]
    }
  }
}
